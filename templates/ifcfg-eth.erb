DEVICE=<%= interface %>
BOOTPROTO=<%= bootproto %>
<% if @macaddress != '' %>HWADDR=<%= macaddress %>
<% end -%>
ONBOOT=<%= onboot %>
HOTPLUG=<%= onboot %>
TYPE=Ethernet
IPADDR=<%= ipaddress %>
NETMASK=<%= netmask %>
<% if @gateway != '' %>GATEWAY=<%= gateway %>
<% end -%>
<% if @mtu != '' %>MTU=<%= mtu %>
<% end -%>
<% if @bonding_opts != '' %>BONDING_OPTS="<%= bonding_opts %>"
<% end -%>
<% if @ethtool_opts != '' %>ETHTOOL_OPTS="<%= ethtool_opts %>"
<% end -%>
<% if @peerdns == true %>PEERDNS=yes<% else %>PEERDNS=no
<% if @dns1_real != '' %>DNS1=<%= dns1_real %>
<% end -%>
<% if @dns2_real != '' %>DNS2=<%= dns2_real %>
<% end -%>
<% if @domain != '' %>DOMAIN="<%= domain %>"
<% end -%>
<% end -%>
NM_CONTROLLED=no
