DEVICE=<%= interface %>
BOOTPROTO=<%= bootproto %>
ONPARENT=<%= onparent %>
TYPE=Ethernet
<% if !ipaddress.empty? %>IPADDR=<%= ipaddress %>
<% end -%>
<% if !netmask.empty? %>NETMASK=<%= netmask %>
<% end -%>
<% if !gateway.empty? %>GATEWAY=<%= gateway %>
<% end -%>
<% if !peerdns.empty? %><% if dns1.empty? %><% if dns2.empty? %>PEERDNS=no
<% else %>PEERDNS=yes
DNS1=<%= dns2 %>
<% end -%><% else %>PEERDNS=yes
DNS1=<%= dns1 %>
<% if !dns2.empty? %>DNS2=<%= dns2 %>
<% end -%>
<% end -%>
<% end -%>
